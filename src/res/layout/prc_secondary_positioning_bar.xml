<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.prc.contact.common.widget.SecondaryPositioningBarView
        android:id="@id/secondary_positioning_bar"
        android:visibility="gone"
        android:layout_width="@dimen/secondary_positioning_bar_width"
        android:layout_centerVertical="true"
        android:layout_marginEnd="@dimen/secondary_positioning_bar_margin_end"
        app:bladeIndicator="@drawable/prc_secondary_positioning_bar_indicator"
        app:bladeIndicatorHeight="@dimen/secondary_positioning_bar_indicator_height"
        app:bladeIndicatorWidth="@dimen/secondary_positioning_bar_indicator_width"
        app:bladeoffsety="5.0dip"
        app:disableSectionColor="@color/dialer_secondarybar_text_color"
        app:dotFontSize="@dimen/secondary_positioning_bar_section_dot_font_size"
        app:enableSectionColor="@color/dialer_secondarybar_text_color"
        app:favoriteSectionSize="@dimen/secondary_positioning_bar_favorite_section_size"
        app:sectionFontSize="@dimen/secondary_positioning_bar_section_font_size"
        app:selectColor="@color/secondary_positioning_bar_section_selected_color"
        app:selectFontSize="@dimen/secondary_positioning_bar_section_selected_font_size"
        style="@style/SecondaryPositioningBarViewStyle" />
</merge>